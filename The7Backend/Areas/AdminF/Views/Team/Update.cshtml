@model Team
@{
    ViewData["Title"] = "Create";
    Layout = "~/Areas/AdminF/Views/Shared/_AdminLayout.cshtml";
}
<div class="container">
    <div class="row">
        <div class="col=md-12">
             <form class="forms-sample" method="post" enctype="multipart/form-data">
                    <div class="form-group" >
                      <label asp-for="Photo">Photo</label>
                      <input asp-for="Photo"  class="form-control"   accept="image/*">
                      <span asp-validation-for="Photo"></span>
                    </div>
                    
                    <div class="form-group">
                      <label asp-for="Profession">Subtitle</label>
                      <input asp-for="Profession"  class="form-control"  placeholder="Subtitle">
                      <span asp-validation-for="Profession"></span>

                    </div> 
                       <div class="form-group">
                      <label asp-for="Author">Desc</label>
                      <input asp-for="Author"  class="form-control"  placeholder="author">
                      <span asp-validation-for="Author"></span>

                    </div> 
                    <a asp-controller="team" asp-action="Index" class="btn btn-primary" >Update</a>
                  </form>
        </div>
       
    </div>
</div>